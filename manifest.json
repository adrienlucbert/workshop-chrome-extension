{
    "name": "Epitech Watch",
    "version": "1.0",
    "description": "Watch your notifications from Epitech's intranet",
    "manifest_version": 2,
    "permissions": [
        "https://intra.epitech.eu/*",
        "alarms",
        "storage"
    ],
    "browser_action": {
        "default_title": "Epitech Watch",
        "default_popup": "src/templates/popup.html"
    },
    "background": {
        "page": "src/templates/background.html"
    }
}